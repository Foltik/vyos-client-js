{
  "name": "vyos-client",
  "version": "0.2.1",
  "description": "VyOS HTTP API Client",
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "doc": "typedoc --out docs src/index.ts",
    "repl": "node --experimental-repl-await repl.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint && npm run doc",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Foltik/vyos-client-node.git"
  },
  "keywords": [
    "vyos",
    "api-client"
  ],
  "author": "Jack Foltz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Foltik/vyos-client-node/issues"
  },
  "homepage": "https://github.com/Foltik/vyos-client-node#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.20.25",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "form-data": "^4.0.0"
  }
}
